application: kweblogo [
  documentation: "Generation of sequence logos using WebLogo"
  groups: "Nucleic:Composition"
  embassy: "kbws"
]

section: input [
  information: "Input section"
  type: "page"
]

  seqall: seqall [
    parameter: "Y"
    type: "gapany"
  ]

endsection: input

section: advanced [
  information: "Advanced section"
  type: "page"
]
  string: format [
    additional: "Y"
    default: "png"
    information: "output file format"
  ]

  string: filename [
    parameter: "Y"
    default: "kweblogo.$(format)"
    information: "Output file name"
  ]

endsection: advanced
